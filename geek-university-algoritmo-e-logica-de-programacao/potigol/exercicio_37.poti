var vetor = Lista.mutavel(0, 0.0)

imprima "Informe o codigo: "
codigo = leia_inteiro

se codigo <> 0 entao
    para i de 1 ate 5 faca
        imprima "Informe um valor {i}/5: "
        valor = leia_real
        vetor := vetor + [valor]
    fim
    
    se codigo == 1 entao 
        para valor em vetor faca
            escreva valor
        fim
    fim
    
    se codigo == 2 entao 
        para i de 5 ate 1 passo -1 faca
            escreva vetor[i]
        fim
    fim
fim